{
  "name": "@hyrious/marshal",
  "version": "0.1.0",
  "description": "Ruby marshal for the browser and node.js",
  "repository": "hyrious/marshal",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build:type": "rollup src/index.ts -p dts -o dist/index.d.ts",
    "build:js": "node --experimental-json-modules scripts/build.mjs",
    "build": "concurrently npm:build:*"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "ruby",
    "marshal",
    "serialize"
  ],
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/parser": "^4.25.0",
    "concurrently": "^6.2.0",
    "esbuild": "^0.12.5",
    "eslint": "^7.27.0",
    "eslint-plugin-unicorn": "^32.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.3",
    "rollup-plugin-dts": "^3.0.2"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": "plugin:unicorn/recommended",
    "ignorePatterns": "dist"
  }
}
