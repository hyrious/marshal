    "marshal",
    "serialize"
  ],
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@hyrious/dts": "^0.1.9",
    "@hyrious/esbuild-dev": "^0.10.4",
    "@types/node": "^18.x",
    "esbuild": "^0.19.2",
    "is-plain-obj": "^4.1.0",
    "pretty-bytes": "^6.1.1",
    "rollup": "^3.28.1",
    "typescript": "^5.2.2",
    "uvu": "^0.5.6"
  }
