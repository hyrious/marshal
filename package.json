{
  "name": "@hyrious/marshal",
  "version": "0.1.5",
  "description": "Ruby marshal for the browser and node.js",
  "repository": "hyrious/marshal",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.iife.js",
  "jsdelivr": "dist/index.iife.js",
  "files": [
    "src",
    "dist"
  ],
  "exports": {
    "node": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "default": "./dist/index.mjs"
  },
  "types": "src/index.ts",
  "scripts": {
    "build": "esbuild-dev scripts/build.ts",
    "test": "esbuild-dev test/index.ts",
    "wip": "esbuild-dev test/wip.ts"
  },
  "engines": {
    "node": ">=12.2.0"
  },
  "keywords": [
    "ruby",
    "marshal",
    "serialize"
  ],
  "author": "hyrious <hyrious@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "@hyrious/esbuild-dev": "^0.7.0",
    "@types/node": "^17.0.2",
    "esbuild": "^0.14.7",
    "uvu": "^0.5.2"
  }
}
