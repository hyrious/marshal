{
  "name": "@hyrious/marshal",
  "version": "0.2.5",
  "description": "Ruby marshal for the browser and node.js",
  "repository": "hyrious/marshal",
  "main": "dist/marshal.js",
  "module": "dist/marshal.mjs",
  "unpkg": "dist/marshal.iife.js",
  "jsdelivr": "dist/marshal.iife.js",
  "files": [
    "src",
    "dist"
  ],
