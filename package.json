    "test": "esbuild-dev test/index.ts"
  },
  "devDependencies": {
    "@hyrious/dts": "^0.2.0",
    "@hyrious/esbuild-dev": "^0.10.5",
    "@types/node": "^20.10.5",
    "esbuild": "^0.19.10",
    "is-plain-obj": "^4.1.0",
    "pretty-bytes": "^6.1.1",
    "rollup": "^4.9.1",
    "typescript": "^5.3.3",
    "uvu": "^0.5.6"
